<?php
//for each loop is used to interate over arrays
$names = array("astra", "rai", "star");
//loop through as elements in the array $names
foreach($names as $value){
	//output each value of each element in array on a new line
	//in echo statement, note the full stop after $value variable before the \m
	echo $value . "\n";
}	
/*
Output:
astra
rai
stra
*/
?>
